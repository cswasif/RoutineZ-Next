{
  "name": "routinez-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "npm run clean && next build",
    "clean": "node -e \"const fs=require('fs'); const path=require('path'); ['.next','.cache','out'].forEach(dir=>{if(fs.existsSync(dir)){fs.rmSync(dir,{recursive:true,force:true})}})\"",
    "start": "next start",
    "lint": "eslint . --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "analyze": "ANALYZE=true next build"
  },
  "dependencies": {
    "@next/font": "^14.1.0",
